{
  "name": "@cododel/alto",
  "version": "0.1.2",
  "description": "A powerful CLI tool for directus",
  "keywords": [
    "cli",
    "typescript",
    "directus",
    "code-generation"
  ],
  "author": "Alexander Smith <websmithcode@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cododel/alto.git"
  },
  "bugs": {
    "url": "https://github.com/cododel/alto/issues"
  },
  "homepage": "https://github.com/cododel/alto#readme",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "bin": {
    "alto": "dist/index.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./cli": "./dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "alto": "bun ./src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node --minify --external consola --external enquirer --external execa --external yargs",
    "prepublishOnly": "bun run build",
    "test:install": "./scripts/test-install.sh",
    "test:uninstall": "./scripts/test-uninstall.sh",
    "test:full-cycle": "./scripts/test-full-cycle.sh"
  },
  "workspaces": [
    "src/commands/*"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "@types/execa": "^2.0.2",
    "@types/yargs": "^17.0.33"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "consola": "^3.4.2",
    "enquirer": "^2.4.1",
    "execa": "^9.6.0",
    "yargs": "^18.0.0"
  }
}